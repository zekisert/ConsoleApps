using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConditionalsIf
{
    class Program
    {
        static void Main(string[] args)
        {
            int kontrolEt1 = 15;
            int kontrolEt2 = 20;

            if (kontrolEt1 == kontrolEt2)
            {
                Console.WriteLine("Eğer");
            }
            else if (kontrolEt1 > kontrolEt2)
            {
                Console.WriteLine("Değilse eğer");
            }
            else
            {
                Console.WriteLine("Değilse");
            }
            Console.ReadLine();
        }
    }
}
